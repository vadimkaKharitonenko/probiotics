{
  "compilerOptions": {
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": false,
    "strictBindCallApply": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  }
}
